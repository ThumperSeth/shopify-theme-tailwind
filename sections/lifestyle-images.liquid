<div class="jacobs-carousel">
  <div class="main-carousel" data-flickity='{ "cellAlign": "center", "contain": true, "freeScroll": true, "wrapAround": true, "lazyLoad": 5 }'>
    <div class="carousel-cell">
      <div class="product-video-content w-full p-0" style="height: 512px; width:1024px">
            <iframe class="w-full h-full" src="{{ product.metafields.custom-video.custom_video | replace: '/watch?v=', '/embed/' }}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
    </div>
    <div class="carousel-cell">
       {% for pic in productPictures %}
        	{% if pic.alt contains "lifestyle" %}
              <div id="thumby-{{ forloop.index }}" class=" block w-auto">
                <img class="thumby w-full h-full" style="width:512px; height:512px;" src="{{ pic.src | img_url: "1024x" }}" alt="{{ pic.alt }}" onClick="swapImages('{{ pic.src | img_url: "1024x1024" }}');" />
              </div>
       		{% endif %}
        {% endfor %}
    </div>
   
  </div>
</div>
</section>

{% schema %}
  {
    "name": "Lifestyle Images",
    "settings": []
  }
{% endschema %}
  
  
  
