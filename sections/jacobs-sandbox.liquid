<section class="section">
<div class="jacobs-carousel">
  <div class="main-carousel" data-flickity='{ "cellAlign": "center", "contain": true, "freeScroll": true, "wrapAround": true, "lazyLoad": 5 }'>
    <div class="carousel-cell">
      <div class="product-video-content w-full p-0" style="height: 512px; width:1024px">
            <iframe class="w-full h-full" src="{{ product.metafields.custom-video.custom_video | replace: '/watch?v=', '/embed/' }}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
    </div>
    <div class="carousel-cell">
      <img data-flickity-lazyload="https://cdn.shopify.com/s/files/1/0258/5424/6963/files/winches_category_card_500x500.jpg?v=1640044491" />
    </div>
    <div class="carousel-cell">
      <img data-flickity-lazyload="https://cdn.shopify.com/s/files/1/0258/5424/6963/files/roofs_category_card_500x500.jpg?v=1640044492" />
    </div>
    <div class="carousel-cell">
      <img data-flickity-lazyload="https://cdn.shopify.com/s/files/1/0258/5424/6963/files/doors_category_card_500x500.jpg?v=1640044492" />
    </div>
    <div class="carousel-cell">
      <img data-flickity-lazyload="https://cdn.shopify.com/s/files/1/0258/5424/6963/files/cages_category_card_500x500.jpg?v=1640044492" />
    </div>
  </div>
</div>
</section>

